{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net7.0/csharp-studies.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "logging": {
                "diagnostics": {
                    "log": {
                        "protocolMessages": false
                    }
                }
            },
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}
